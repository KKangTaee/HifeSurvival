<?xml version="1.0" encoding="utf-8" ?>
<PDL>
	<packet name="C_Chat">
		<string name="chat"/>
	</packet>

	<packet name="S_Chat">
		<int name="playerId"/>
		<string name="chat"/>
	</packet>

	<packet name="C_JoinToGame">
		<string name ="userId"/>
		<string name ="userName"/>
	</packet>

	<packet name = "S_JoinToGame">
		<list name = "JoinPlayer">
			<string name = "userId"/>
			<string name = "userName"/>
			<int name = "playerId"/>
			<int name ="heroId"/>
		</list>
		<int name="roomId"/>
	</packet>

	<packet name ="S_LeaveToGame">
		<string name = "userId"/>
		<int name ="playerId"/>
	</packet>

	<packet name="CS_SelectHero">
		<int  name="playerId"/>
		<int  name="heroId"/>
	</packet>

	<packet name="CS_ReadyToGame">
		<int name ="playerId"/>
	</packet>

	<packet name="S_Countdown">
		<int name ="countdownSec"/>
	</packet>

	<packet name="S_StartGame">
		<list name="Player">
			<int name="playerId"/>
			<int name="heroId"/>
		</list>
		<list name="Monster">
			<int name ="monsterId"/>
			<int name ="monsterType"/>
			<int name ="groupId"/>
			<int name ="subId"/>
		</list>
	</packet>

	<packet name ="CS_Attack">
		<bool name ="toIdIsPlayer"/>
		<int name ="toId"/>
		<int name ="fromId"/>
		<int name ="damageValue"/>
	</packet>

	<packet name ="CS_Move">
		<int name ="targetId"/>
		<bool name ="isPlayer"/>
		<struct_Vec3 name ="pos"/>
		<struct_Vec3 name ="dir"/>
		<float name ="speed"/>
	</packet>
	
	<packet name ="CS_StopMove">
		<int name ="targetId"/>
		<bool name ="isPlayer"/>
		<struct_Vec3 name ="pos"/>
		<struct_Vec3 name ="dir"/>
	</packet>

	<packet name ="S_Dead">
		<int name ="toId"/>
		<int name ="fromId"/>
		<bool name ="toIdIsPlayer"/>
		<int name ="respawnTime"/>
	</packet>

	<packet name ="S_Respawn">
		<int name ="targetId"/>
		<bool name ="isPlayer"/>
		<struct_Vec3 name ="pos"/>
	</packet>

	<struct name="Vec3">
		<float name ="x"/>
		<float name ="y"/>
		<float name ="z"/>
	</struct>

</PDL>